#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../.store/http-server@14.1.1/node_modules/http-server/bin/http-server" "$@"
  ret=$?
else 
  node  "$basedir/../.store/http-server@14.1.1/node_modules/http-server/bin/http-server" "$@"
  ret=$?
fi
exit $ret
